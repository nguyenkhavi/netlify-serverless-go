{
  "name": "landing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/landing",
  "projectType": "application",
  "targets": {
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node prebuild.js"
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "dependsOn": [
        "prebuild"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/landing",
        "generateLockfile": true
      },
      "configurations": {
        "development": {
          "outputPath": "apps/landing"
        },
        "production": {}
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "landing:build",
        "dev": true,
        "port": 3400
      },
      "configurations": {
        "development": {
          "buildTarget": "landing:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "landing:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "landing:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/landing/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/landing/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
